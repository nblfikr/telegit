{
  "name": "flyingit",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon -e ts --watch src --exec \"node --loader @esbuild-kit/esm-loader src/index.ts\"",
    "build": "tsc --noEmit && node build.js",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prett": "prettier --config .prettierrc 'src/**/*.ts' 'test/**/*.ts' --write",
    "prepare": "husky install",
    "test": "vitest --config ./vitest.config.ts run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@octokit/webhooks-types": "^6.5.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "grammy": "^1.11.2",
    "polka": "^1.0.0-next.22"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.5.0",
    "@grammyjs/types": "^2.9.1",
    "@types/body-parser": "^1.19.2",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitest/coverage-c8": "^0.24.1",
    "axios": "^1.1.3",
    "esbuild": "^0.15.10",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1"
  }
}
